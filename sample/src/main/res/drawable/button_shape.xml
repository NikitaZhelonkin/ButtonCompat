<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
       android:insetLeft="@dimen/button_compat_inset_horizontal_material"
       android:insetTop="@dimen/button_compat_inset_vertical_material"
       android:insetRight="@dimen/button_compat_inset_horizontal_material"
       android:insetBottom="@dimen/button_compat_inset_vertical_material">
    <shape android:shape="rectangle"
           android:tint="?attr/colorButtonNormal">
        <corners android:radius="@dimen/button_compat_corner_material" />
        <solid android:color="@android:color/white" />
        <padding android:left="@dimen/button_compat_padding_horizontal_material"
                 android:top="@dimen/button_compat_padding_vertical_material"
                 android:right="@dimen/button_compat_padding_horizontal_material"
                 android:bottom="@dimen/button_compat_padding_vertical_material" />
    </shape>
</inset>